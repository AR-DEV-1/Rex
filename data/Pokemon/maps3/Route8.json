  "texts": [
    {
      "name": "Route8SuperNerd1Battle",
      "values": [
        {
          "text": "You look good at",
          "type": "text"
        },
        {
          "text": "#MON, but",
          "type": "line"
        },
        {
          "text": "how's your chem?",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8SuperNerd1EndBattle",
      "values": [
        {
          "text": "Ow!",
          "type": "text"
        },
        {
          "text": "Meltdown!",
          "type": "line"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8SuperNerd1AfterBattle",
      "values": [
        {
          "text": "I am better at",
          "type": "text"
        },
        {
          "text": "school than this!",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8Gambler1Battle",
      "values": [
        {
          "text": "All right! Let's",
          "type": "text"
        },
        {
          "text": "roll the dice!",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8Gambler1EndBattle",
      "values": [
        {
          "text": "Drat!",
          "type": "text"
        },
        {
          "text": "Came up short!",
          "type": "line"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8Gambler1AfterBattle",
      "values": [
        {
          "text": "Lady Luck's not",
          "type": "text"
        },
        {
          "text": "with me today!",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8SuperNerd2Battle",
      "values": [
        {
          "text": "You need strategy",
          "type": "text"
        },
        {
          "text": "to win at this!",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8SuperNerd2EndBattle",
      "values": [
        {
          "text": "It's",
          "type": "text"
        },
        {
          "text": "not logical!",
          "type": "line"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8SuperNerd2AfterBattle",
      "values": [
        {
          "text": "Go with GRIMER",
          "type": "text"
        },
        {
          "text": "first...and...",
          "type": "line"
        },
        {
          "text": "...and...then...",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF1Battle",
      "values": [
        {
          "text": "I like NIDORAN, so",
          "type": "text"
        },
        {
          "text": "I collect them!",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF21EndBattle",
      "values": [
        {
          "text": "Why? Why??",
          "type": "text"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF1AfterBattle",
      "values": [
        {
          "text": "When #MON grow",
          "type": "text"
        },
        {
          "text": "up they get ugly!",
          "type": "line"
        },
        {
          "text": "They shouldn't",
          "type": "cont"
        },
        {
          "text": "evolve!",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8SuperNerd3Battle",
      "values": [
        {
          "text": "School is fun, but",
          "type": "text"
        },
        {
          "text": "so are #MON.",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8SuperNerd3EndBattle",
      "values": [
        {
          "text": "I'll",
          "type": "text"
        },
        {
          "text": "stay with school.",
          "type": "line"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8SuperNerd3AfterBattle",
      "values": [
        {
          "text": "We're stuck here",
          "type": "text"
        },
        {
          "text": "because of the",
          "type": "line"
        },
        {
          "text": "gates at SAFFRON.",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF2Battle",
      "values": [
        {
          "text": "MEOWTH is so cute,",
          "type": "text"
        },
        {
          "text": "meow, meow, meow!",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF2EndBattle",
      "values": [
        {
          "text": "Meow!",
          "type": "text"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF2AfterBattle",
      "values": [
        {
          "text": "I think PIDGEY",
          "type": "text"
        },
        {
          "text": "and RATTATA",
          "type": "line"
        },
        {
          "text": "are cute too!",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF3Battle",
      "values": [
        {
          "text": "We must look",
          "type": "text"
        },
        {
          "text": "silly standing",
          "type": "line"
        },
        {
          "text": "here like this!",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF3EndBattle",
      "values": [
        {
          "text": "Look what",
          "type": "text"
        },
        {
          "text": "you did!",
          "type": "line"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF3AfterBattle",
      "values": [
        {
          "text": "SAFFRON's gate",
          "type": "text"
        },
        {
          "text": "keeper won't let",
          "type": "line"
        },
        {
          "text": "us through.",
          "type": "cont"
        },
        {
          "text": "He's so mean!",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8Gambler2Battle",
      "values": [
        {
          "text": "I'm a rambling,",
          "type": "text"
        },
        {
          "text": "gambling dude!",
          "type": "line"
        }
      ]
    },
    {
      "name": "Route8Gambler2EndBattle",
      "values": [
        {
          "text": "Missed",
          "type": "text"
        },
        {
          "text": "the big score!",
          "type": "line"
        },
        {
          "text": "prompt",
          "type": "prompt"
        }
      ]
    },
    {
      "name": "Route8Gambler2AfterBattle",
      "values": [
        {
          "text": "Gambling and",
          "type": "text"
        },
        {
          "text": "#MON are like",
          "type": "line"
        },
        {
          "text": "eating peanuts!",
          "type": "cont"
        },
        {
          "text": "Just can't stop!",
          "type": "cont"
        }
      ]
    },
    {
      "name": "Route8CooltrainerF4Battle",
      "values": [
        {
          "text": "What's a cute,",
          "type": "text"
        },
        {
          "text": "round and fluffy",
          "type": "line"
        },
        {
          "text": "#MON?",
          "type": "cont"
