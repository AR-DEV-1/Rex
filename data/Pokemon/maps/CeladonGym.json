  "texts": [
    {
      "name": "CeladonGymErikaPreBattle",
      "values": [
        {
          "text": "Hello. Lovely",
          "type": "text"
        },
        {
          "text": "weather isn't it?",
          "type": "line"
        },
        {
          "text": "It's so pleasant.",
          "type": "cont"
        },
        {
          "text": "...Oh dear...",
          "type": "para"
        },
        {
          "text": "I must have dozed",
          "type": "line"
        },
        {
          "text": "off. Welcome.",
          "type": "cont"
        },
        {
          "text": "My name is ERIKA.",
          "type": "para"
        },
        {
          "text": "I am the LEADER",
          "type": "line"
        },
        {
          "text": "of CELADON GYM.",
          "type": "cont"
        },
        {
          "text": "I teach the art of",
          "type": "para"
        },
        {
          "text": "flower arranging.",
          "type": "line"
        },
        {
          "text": "My #MON are of",
          "type": "cont"
        },
        {
          "text": "the grass-type.",
          "type": "cont"
        },
        {
          "text": "Oh, I'm sorry, I",
          "type": "para"
        },
        {
          "text": "had no idea that",
          "type": "line"
        },
        {
          "text": "you wished to",
          "type": "cont"
        },
        {
          "text": "challenge me.",
          "type": "cont"
        },
        {
          "text": "Very well, but I",
          "type": "para"
        },
        {
          "text": "shall not lose.",
          "type": "line"
        }
      ]
    },
    {
      "name": "CeladonGymErikaReceivedRainbowBadge",
